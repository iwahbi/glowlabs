<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <%-include('./partials/menu')%>

    <table id="table">
        <thead>
            <th>
                Nom
            </th>
            <th>
                Prenom
            </th>
        </thead>
        <tbody>
            <% users.forEach(function(user){ %>
                <tr>
                    <td><%= user.nom %></td>
                    <td><%= user.prenom %></td>
                </tr>
                <% });%>
        </tbody>
    </table>
</body>
</html>